##############################################################################

sub show(+) {
    require Dumpvalue;
    state $p = new Dumpvalue::
      tick => q("),
      compactDump => 1,
      veryCompact => 1,
    ;
    dumpValue $p @_;
}

##############################################################################

my %hash; @hash{@array} = (1) x scalar(@array);

##############################################################################
